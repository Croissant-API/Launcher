{
  "applicationId": "croissant-launcher",
  "defaultMode": "window",
  "documentRoot": "/resources/app",
  "url": "https://croissant-api.fr/launcher/home",
  "modes": {
    "window": {
      "width": 800,
      "height": 600,
      "resizable": true,
      "fullscreen": false,
      "maximize": true,
      "exitProcessOnClose": true,
      "icon": "/resources/icons/icon.png",
      "title": "Croissant Launcher"
    }
  },
  "cli": {
    "binaryName": "croissant-launcher"
  },
  "extensions": [],
  "platform": {
    "windows": {
      "singleInstance": true
    }
  }
}
